#include <stdio.h>
#include <stdlib.h>

int calSoma(int vet[], int tam, int i){
	
	int soma = 0;
	
	for(i=0;i<tam;i++){
		soma = soma + vet[i];
	}
	
	return soma;
	
}//fim do calSoma

int main(){
	
	int n, i, soma;
	
	printf("Qual o tamanho do seu vetor? (Ate 100):  ");
	scanf("%d",&n);
	
	if(n<=0 || n>100){
		printf("Tamanho inválido, o tamanho deverá ter de 1 a 100");
	}
	
	int vet[n];
	
	printf("Digite os valores do vetor: \n");
	
	for(i=0;i<n;i++){
		printf("Valor %d: ",i+1);
		scanf("%d", &vet[i]);
	}
	
	soma = calSoma(vet, n, i);
	
	printf("A soma dos elementos e: %d\n",soma);
	
	return 0;
	
	
}
